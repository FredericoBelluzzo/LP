	<queryString language="SQL">
		<![CDATA[SELECT
     funcionario.`codFuncionario` AS funcionario_codFuncionario,
     funcionario.`nome` AS funcionario_nome,
     funcionario.`telefone` AS funcionario_telefone,
     funcionario.`rg` AS funcionario_rg,
